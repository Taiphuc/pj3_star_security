@model IEnumerable<eProject3.Model.DAL.Business>
@if ((int)ViewBag.Count > 0)
{
    foreach (var item in Model)
    {
        var url = "/Businesses/Detail/" + item.Id;
        <div class="col-md-3 product-left">
            <div class="p-one simpleCart_shelfItem" style="height:410px;">
                <a href="@url">
                    <img src="@item.UrlImage" width="223" height="232" alt="" />
                    <div class="mask">
                        <span>Xem chi tiết</span>
                    </div>
                </a>
                <h4>@item.Name</h4>
            </div>
        </div>
    }
}
